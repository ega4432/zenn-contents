{
  "filters": {
    "comments": true
  },
  "rules": {
    "preset-ja-spacing": {
      "ja-space-between-half-and-full-width": {
        "space": "always"
      }
    },
    "preset-ja-technical-writing": true,
    "prh": true,
    "spellcheck-tech-word": true,
    "no-double-negative-ja": true,
    "no-dropping-the-ra": true,
    "no-doubled-joshi": {
        "min_interval": 1
    },
    "no-unmatched-pair": true
  }
}
